<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>loginBean</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.frontend.login.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>actualizarLocalesBean</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.frontend.processes.ActualizarLocalesBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>bajaPoisBean</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.frontend.processes.BajaPOIsBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>agregarAccionesBean</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.frontend.processes.AgregarAccionesBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>accionesDeConsultaBean</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.frontend.acciones.AccionesDeConsultaBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>busquedaBean</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.frontend.busqueda.BusquedaBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>basicView</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.frontend.busqueda.BasicView</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>poi</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.frontend.busqueda.PoiService</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>historialBean</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.frontend.historial.HistorialBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>banco</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.db.poi.Banco</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>menuBean</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.frontend.welcome.MenuBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>usuario</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.autentification.Usuario</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>rol</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.autentification.Rol</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>accion</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.autentification.Accion</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>altaPOIsMaskView</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.frontend.abmPOIs.altaPOIsMaskView</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>nodoServicio</managed-bean-name>
		<managed-bean-class>ar.edu.utn.dds.grupouno.abmc.poi.NodoServicio</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>

	<navigation-rule>
		<display-name>login.xhtml</display-name>
			<from-view-id>/login.xhtml</from-view-id>
				<navigation-case>
					<from-outcome>loginPage</from-outcome>
					<to-view-id>/login.xhtml</to-view-id>
				</navigation-case>
    			<navigation-case>
        			<from-outcome>failure</from-outcome>
        			<to-view-id>/login.xhtml</to-view-id>
    			</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>processes.xhtml</display-name>
		<from-view-id>/processes.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{processes.actualizarlocales}</from-action>
			<to-view-id>/actualizarlocales.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>processes.xhtml</display-name>
		<from-view-id>/processes.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{processes.procesomultiple}</from-action>
			<to-view-id>/procesomultiple.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>processes.xhtml</display-name>
		<from-view-id>/processes.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{processes.bajapois}</from-action>
			<to-view-id>/bajapois.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>processes.xhtml</display-name>
		<from-view-id>/processes.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{processes.agregaracciones}</from-action>
			<to-view-id>/agregaracciones.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>success</from-outcome>
			<to-view-id>/welcome.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>configuracionAcciones.xhtml</display-name>
		<from-view-id>/configuracionAcciones.xhtml</from-view-id>
				<navigation-case>
        			<from-outcome>cancel</from-outcome>
        			<to-view-id>/welcome.xhtml</to-view-id>
        			<redirect />
    			</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>actualizarlocales.xhtml</display-name>
		<from-view-id>/actualizarlocales.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/welcome.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>procesomultiple.xhtml</display-name>
		<from-view-id>/procesomultiple.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/welcome.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>agregaracciones.xhtml</display-name>
		<from-view-id>/agregaracciones.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/welcome.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>bajapois.xhtml</display-name>
		<from-view-id>/bajapois.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>index</from-outcome>
			<to-view-id>/welcome.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>welcome.xhtml</display-name>
		<from-view-id>/welcome.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>processes</from-outcome>
			<to-view-id>/processes.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>welcome.xhtml</display-name>
		<from-view-id>/welcome.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>busqueda</from-outcome>
			<to-view-id>/busqueda.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>welcome.xhtml</display-name>
		<from-view-id>/welcome.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>historial</from-outcome>
			<to-view-id>/historial.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>welcome.xhtml</display-name>
		<from-view-id>/welcome.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>configuracionAcciones</from-outcome>
			<to-view-id>/configuracionAcciones.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>welcome.xhtml</display-name>
		<from-view-id>/welcome.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>abmpois</from-outcome>
			<to-view-id>/abmpois.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>abmpois.xhtml</display-name>
		<from-view-id>/abmpois.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>poisAlta</from-outcome>
			<to-view-id>/poisAlta.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<lifecycle>
		<phase-listener>ar.edu.utn.dds.grupouno.frontend.security.AuthorizationListener</phase-listener>
	</lifecycle>
</faces-config>
